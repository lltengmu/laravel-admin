{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": [
    "vite.config.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "vite/**/*.ts",
    "mock/**/*.ts",
    "types/**/*.d.ts"
  ]
}